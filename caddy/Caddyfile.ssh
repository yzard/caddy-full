{
    log default {
        output stdout
        format json
        include http.log.access
    }

    http_port 80
    https_port 8443

    layer4 {
        :443 {
            @tls tls
            route @tls {
                proxy 127.0.0.1:8443
            }
{{SSH_BLOCK}}
        }
    }
}

:80 {
    redir https://{host}{uri} permanent
}

import default.d/*
import conf.d/*
